{
  "manifest_version": 3,
  "name": "Web Scraper Extension",
  "version": "1.0",
  "description": "Scrape data from websites",
  "permissions": ["activeTab", "storage", "tabs"],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "64": "icons/icon64.png",
      "128": "icons/icon128.png"
    }
  },

  "background": {
    "service_worker": "src/background.js"
  },

  "icons": {
    "16": "icons/icon16.png",
    "64": "icons/icon64.png",
    "128": "icons/icon128.png"
  }
}
